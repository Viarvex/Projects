#include <bits/stdc++.h>
#define ll = long long

int main() {

    char op; // operator
    std::vector<long long> nums;
    long long plac;
    nums.push_back(plac);

    std::cout << "ENTER OPERATOR: (+, -, *, /): ";
    std::cin >> op;

    std::cout << "ENTER NUMBERS: \n";
    std::cin >> plac;

    switch(op) { // switch operation
        case '+': std::cout << nums[0] + nums[1]; break;
        case '-': std::cout << nums[0] - nums[1]; break;
        case '*': std::cout << nums[0] * nums[1]; break;
        case '/': std::cout << nums[0] / nums[1]; break;
        default: std::cout << "ERROR"; break;
    }

    return 0;
}
