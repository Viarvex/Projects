#include <bits/stdc++.h>
using vi = std::vector<int>; using vs = std::vector<std::string>; using txt = std::string; using vl = std::vector<long long>;



int main() {
    std::ios::sync_with_stdio(false);
    std::cin.tie(nullptr);

    char op; // operator
    float num1, num2;

    std::cout << "ENTER OPERATOR: (+, -, *, /): \n";
    std::cin >> op;

    std::cout << "ENTER NUMBERS: \n";
    std::cin >> num1 >> num2;

    switch(op) { // switch operation
        case '+': std::cout << num1 + num2; break;
        case '-': std::cout << num1 - num2; break;
        case '*': std::cout << num1 * num2; break;
        case '/': std::cout << num1 / num2; break;
        default: std::cout << "ERROR"; break;
    }

    return 0;
}
